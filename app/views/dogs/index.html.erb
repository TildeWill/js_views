<h1>All the Dogs</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Breed</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @dogs.each do |dog| %>
      <tr>
        <td><%= dog.name %></td>
        <td><%= dog.breed %></td>
        <td><%= link_to("Edit", edit_dog_path(dog)) %></td>
        <td><%= link_to("Delete", dog_path(dog), method: :delete) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to("New Dog", new_dog_path) %>
